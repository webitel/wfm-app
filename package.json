{
  "name": "@webitel/wfm",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "utils:up-lib": "npm up @webitel/ui-sdk @webitel/ui-datalist && npm ls @webitel/ui-sdk @webitel/ui-datalist",
    "utils:link-libs": "npm link @webitel/api-services @webitel/ui-sdk @webitel/ui-datalist && npm ls @webitel/api-services @webitel/ui-sdk @webitel/ui-datalist",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0",
    "@webitel/api-services": "^0.0.73",
    "@webitel/ui-datalist": "^1.0.84",
    "@webitel/ui-sdk": "~25.12",
    "axios": "^1.12.2",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0",
    "webitel-sdk": "^25.8.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.11.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "~10.4.0",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.11.0",
    "prettier": "3.6.2",
    "tslib": "^2.7.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^7.2.1",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-svg-sprite": "=0.6.3",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.1.10"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.24.0",
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "overrides": {
    "vue-tsc": "^2.1.10",
    "vite": "^7.2.1"
  }
}
